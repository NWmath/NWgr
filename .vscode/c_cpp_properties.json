{
    "env": {
        "myDefaultIncludePath": [
            "${workspaceFolder}",
            "${workspaceFolder}/include",
            "${workspaceFolder}/include/**"
        ],
        "myCompilerPath": "/usr/local/bin/g++-10"
    },
    "configurations": [
        {
            "name": "Mac",
            "configurationProvider": "ms-vscode.cmake-tools",
            "intelliSenseMode": "gcc-x86",
            "includePath": [
                "${workspaceFolder}",
                "${workspaceFolder}/include",
                "${workspaceFolder}/include/**",
                "${workspaceFolder}/../NWut/include",
                "/usr/local/include"
            ],
            "compilerPath": "/usr/local/bin/g++-10",
            "cppStandard": "c++20",
            "cStandard": "c11"
        }
    ],
    "version": 4
}